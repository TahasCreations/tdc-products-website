{
  "name": "@tdc/web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "reset:dry": "tsx prisma/factory-reset.ts --dry",
    "reset:all": "FACTORY_RESET=I_UNDERSTAND FACTORY_RESET_CONFIRM=TDC_MARKET tsx prisma/factory-reset.ts",
    "reset:force": "FACTORY_RESET=I_UNDERSTAND FACTORY_RESET_CONFIRM=TDC_MARKET tsx prisma/factory-reset.ts --force",
    "demo:status": "set ALLOW_DEMO_PURGE=I_UNDERSTAND && tsx prisma/purge-demo.ts --dry",
    "demo:purge": "set ALLOW_DEMO_PURGE=I_UNDERSTAND && tsx prisma/purge-demo.ts",
    "demo:purge:force": "set ALLOW_DEMO_PURGE=I_UNDERSTAND && tsx prisma/purge-demo.ts --force",
    "seed": "tsx prisma/seed.ts",
    "seed:demo": "set SEED_DEMO=true && tsx prisma/seed.ts",
    "db:check": "tsx scripts/check-sqlite-demo.ts",
    "db:clean": "tsx scripts/clean-sqlite-demo.ts",
    "db:admin": "tsx scripts/create-admin.ts",
    "db:index": "psql -U postgres -d tdc_market -f prisma/migrations/add_performance_indexes.sql",
    "test:load": "k6 run tests/load-test.js",
    "test:load:artillery": "artillery run tests/load-test.yml",
    "analyze": "cross-env ANALYZE=true npm run build",
    "media:index": "tsx scripts/media-indexer.ts",
    "media:index:dry": "tsx scripts/media-indexer.ts --dry-run",
    "media:optimize": "tsx scripts/media-optimizer.ts"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
