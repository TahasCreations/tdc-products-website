{
  "name": "@tdc/infra",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "clean": "rm -rf dist",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:seed": "tsx src/database/seed.ts",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull"
  },
  "dependencies": {
    "@tdc/domain": "workspace:*",
    "@tdc/config": "workspace:*",
    "@supabase/supabase-js": "^2.58.0",
    "@prisma/client": "^6.16.2",
    "prisma": "^6.16.2",
    "redis": "^4.6.12",
    "aws-sdk": "^2.1531.0",
    "stripe": "^14.12.0",
    "zod": "^3.23.8",
    "bcrypt": "^6.0.0",
    "jsonwebtoken": "^9.0.2",
    "@sentry/node": "^7.91.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "ioredis": "^5.3.2",
    "meilisearch": "^0.37.0",
    "bullmq": "^4.15.4"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "typescript": "^5.3.3",
    "eslint": "^8.57.0",
    "vitest": "^1.0.0",
    "tsx": "^4.7.1"
  }
}