# TDC Market Dev Container Environment Variables

# Node Environment
NODE_ENV=development

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/tdc_market
POSTGRES_DB=tdc_market
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

# Redis
REDIS_URL=redis://localhost:6379

# S3 Storage
S3_ENDPOINT=https://s3.amazonaws.com
S3_BUCKET=tdc-market-dev
S3_KEY=your-access-key
S3_SECRET=your-secret-key

# Payment
PAYMENT_MERCHANT_ID=your-merchant-id
PAYMENT_KEY=your-payment-key
PAYMENT_SECRET=your-payment-secret

# Authentication
NEXTAUTH_SECRET=your-nextauth-secret
JWT_SECRET=your-jwt-secret

# MeiliSearch
MEILISEARCH_HOST=http://localhost:7700
MEILISEARCH_KEY=masterKey

# AWS
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_DEFAULT_REGION=us-east-1

# Application URLs
NEXT_PUBLIC_API_URL=http://localhost:3002
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_ADMIN_URL=http://localhost:3001

# Development
TURBO_TOKEN=your-turbo-token
TURBO_TEAM=your-turbo-team

